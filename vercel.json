{
  "rewrites": [
    {
      "source": "/summary",
      "destination": "/app.py/summary"
    },
    {
      "source": "/loop",
      "destination": "/app.py/loop"
    },
    {
      "source": "/(.*)",
      "destination": "/app.py"
    }
  ],
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ]
}
